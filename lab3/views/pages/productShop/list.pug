extends ../../layout

block content
  h3 Products in Shop List



  table(class="table")
    thead
      tr
        th Shop
        th Product
    tbody
      each productShop in productShops
        tr
          td #{productShop.shop}
          td #{productShop.product}
          td
            form(action=`toStorage/${productShop.id}`, method='get')
              button(type='submit') Move to Storage